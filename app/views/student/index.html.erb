<br class="clear">
<div id="index-render-about-wrapper" class="articles-page clearfix student-index-render">
  <div id="index-render-about" class="container faq">
    <!-- <div class="g12"><h2>Відповіді на найбільш часті запитання</h2></div> -->
    <br class="clear">
    <% if @students.blank? %>
      <div class="g12" style="height: 500px;">&nbsp;</div>
    <% else %>
    <div class="g5 alpha mf">
      <div id="index-render-shadow"></div>
      <ul>
        <% @students.each do |faq| %>
          <li><%= link_to faq.question, student_show_path(faq) %></li>
        <% end %>
      </ul>
      <div id="index-render-shadow-rotate"></div>
    </div>


    <div class="g7 omega mf faq-render">
      <p><%= t('variables.students_page.p_text') %></p>
    </div>

    <% end %>



  </div>
</div>
<br class="clear">
